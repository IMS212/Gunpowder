{
  "schemaVersion": 1,
  "id": "gunpowder-base",
  "version": "${version}",
  "name": "Gunpowder",
  "description": "The base implementation of Gunpowder - Unofficial Quilt Version",
  "license": "MIT",
  "icon": "assets/gunpowder/icon.png",
  "environment": "*",
  "entrypoints": {
    "server": [
      "io.github.gunpowder.mod.server.GunpowderServerMod"
    ],
    "client": [
      "io.github.gunpowder.mod.client.GunpowderClientMod"
    ]
  },
  "mixins": [
    "mixins.base.gunpowder.json"
  ],
  "accessWidener": "gunpowder_base.accesswidener",
  "depends": {
    "fabricloader": ">=0.8.9",
    "fabric": "*",
    "fabric-language-kotlin": ">=1.3.50"
  },
  "breaks": {
    "gunpowder-api": "*"
  },
  "custom": {
    "hermes": true
  }
}
